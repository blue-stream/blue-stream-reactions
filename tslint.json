{
  "extends": "tslint-config-airbnb",
  // rules override:
  "rules": {
    "no-increment-decrement" : false,
    "import-name": false, // 23.6,
    "max-line-length": false,  // 18.12   We can debate about that...
    "variable-name": [true, "ban-keywords", "check-format", "allow-snake-case", "allow-leading-underscore", "allow-pascal-case"],
    "ter-indent": [true, 4],
    "ter-prefer-arrow-callback": false
  }
}